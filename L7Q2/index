<script>
	$(document).ready(function () {
		$.getJSON("https://api.um.edu.my/website/events?limit=4", function (response) {

			var items = [];
			
			$.each(response.data, function (key, value) {

				console.log(value);
				if(value.contact_phone==null){
					value.contact_phone='';
				}
				
				if(value.contact_email==null){
					value.contact_email='';
				}
				
				if(value.startdate==value.enddate){
					var displaydate=value.startdate;
				} else {
					var displaydate=value.startdate +" - "+ value.enddate;
				}
				
				if(value.location=="manual"){
					value.location=value.tr_ruang2;
				}
				
				items.push(
					"<div class=\"modal fade\" id=\"myModal"+ value.event_id +"\" role=\"dialog\">" +
						"<div class=\"modal-dialog\">" +
							"<div class=\"modal-content\">" +
								"<div class=\"modal-header\">" +
									"<button type=\"button\" class=\"close\" data-dismiss=\"modal\">&times;</button>" +
									"<h4 class=\"modal-title\">Event Details</h4>" +
								"</div>" +
								"<div class=\"modal-body\" style=\"margin-left: 15px; margin-bottom: 5px;\">" +
									"<table class=\"table table-borderless\">" +
										"<tr>" +
											"<td align=\"left\" width=\"25%\"><b>Title</b></td>" +
											"<td align=\"left\">"+ value.title +"</td>" +
										"</tr>" +
										"<tr>" +
											"<td align=\"left\"><b>Location</b></td>" +
											"<td align=\"left\">"+ value.location +"</td>" +
										"</tr>" +
										"<tr>" +
											"<td align=\"left\"><b>Date</b></td>" +
											"<td align=\"left\">"+ displaydate +"</td>" +
										"</tr>" +
										"<tr>" +
											"<td align=\"left\"><b>Time</b></td>" +
											"<td align=\"left\">"+ value.starttime +" - "+ value.endtime +"</td>" +
										"</tr>" +
										"<tr>" +
											"<td align=\"left\"><b>Description</b></td>" +
											"<td align=\"left\">"+ value.description +"</td>" +
										"</tr>" +
										"<tr>" +
											"<td align=\"left\"><b>Contact</b></td>" +
											"<td align=\"left\">"+ value.contact_name +"<br />"+ value.contact_unit +"<br />"+ value.contact_ptj +"<br />"+ value.contact_email +"<br />"+ value.contact_phone +"</td>" +
										"</tr>" +
									"</table>" +
								"</div>" +
								"<div class=\"modal-footer\">" +
									"<button type=\"button\" class=\"btn btn-default\" data-dismiss=\"modal\">Close</button>" +
								"</div>" +
							"</div>" +
						"</div>" +
					"</div>");
			});

			$("#modal-data").html(items.join(""));
		});
	});    
</script>

